{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src https:; script-src https:'unsafe-inline'; style-src 'self' 'unsafe-inline'; connect-src 'self' https://www.kobis.or.kr; object-src none "
        }
      ]
    }
  ]
}
